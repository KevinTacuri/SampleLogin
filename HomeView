import SwiftUI

struct HomeView: View {
    @Binding var isLoggedin: Bool     var body: some View {
        VStack {
            Text("Welcome to my application!")
                .font(.largeTitle)
                .padding()
       
            Button("Cerrar sesi√≥n", action: logout)
                .buttonStyle(.bordered)
                .padding()
        }
    }  
    func logout() {
        isLoggedin = false  
    }
}
